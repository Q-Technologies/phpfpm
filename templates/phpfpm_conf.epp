;
; File Managed by Puppet
;
[global]
pid = <%= $pid_dir %>/php-fpm.pid
error_log = <%= $log_dir %>/php-fpm.log

[www]
listen = <%= $socket_dir %>/php_fpm.sock
env[HOSTNAME] = $HOSTNAME
<% $pool_ini.each | $key, $value | { -%>
<%= $key %> = <%= $value %>
<% } -%>

include=<%= $phpfpm_pool_dir %>/*.conf

